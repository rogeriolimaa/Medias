	printf ("Insira a sua nota de Educacao Fisica\n");
	scanf ("%d", &o);
	printf("nota:%d\n",o);
	printf ("A sua media final e: %d\n", (a+b+c+d+e+f+g+h+i+o+j+k+l+m+n+o)/15);
	
	if ((a+b+c+d+e+f+g+h+i+j+k+l+m+n+o)/15 >=7)
	
	{printf ("Parabens,voce passou.\n");}
	else
	{printf ("Voce esta de recuperacao.\n");}
	
